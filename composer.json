{
    "name": "bingcool/swoolefy",
    "description": "swoolefy is a HV Framework based on swoole extension, for everyone use it easily!",
    "keywords": [
        "swoolefy",
        "swoole",
        "php",
        "auto relaod",
        "monitor"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "bingcool",
            "email": "2437667702@qq.com",
            "role": "Engineer of php"
        }
    ],
    "bin": [
        "bin/swoolefy"
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "^7.3",
        "ext-swoole" : "^4.8",
        "ext-json": "*",
        "psr/log" : "*",
        "ext-pcntl" : "*",
        "ext-posix" : "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-sockets": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "dragonmantank/cron-expression": "~3.3.1",
        "bingcool/library": "^4.8"
    },
    "autoload": {
        "files": ["src/Constants.php","src/Core/Func/function.php"],
        "psr-4": {
            "Swoolefy\\": "src/"
        }
    },
    "require-dev": {
        "kwn/php-rdkafka-stubs": "@dev",
        "swoole/ide-helper": "@dev"
    }
}
